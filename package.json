{
  "name": "examples-trpc-next-prisma-starter",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prestart": "prisma migrate deploy",
    "build": "next build",
    "start": "next start",
    "start-docker": "prisma migrate deploy && node ./server.js",
    "lint": "eslint --cache src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test-unit": "vitest",
    "test-e2e": "playwright test",
    "test-start": "run-s test-unit test-e2e",
    "postinstall": "pnpm generate",
    "expire-reserved": "NODE_ENV=production tsx src/scripts/expire-reserved-orders.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@paypal/paypal-server-sdk": "^1.0.0",
    "@paypal/react-paypal-js": "^8.8.3",
    "@prisma/client": "^6.7.0",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@tanstack/react-query": "^5.74.7",
    "@trpc/client": "npm:@trpc/client",
    "@trpc/next": "npm:@trpc/next",
    "@trpc/react-query": "npm:@trpc/react-query",
    "@trpc/server": "npm:@trpc/server",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.507.0",
    "next": "^15.1.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "superjson": "^1.12.4",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.5",
    "@types/react": "^19.1.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^6.7.0",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "version": "11.1.2"
}
